{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-sales-shreyas"
		},
		"shreyasgithub_ls_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'shreyasgithub_ls'"
		},
		"storagedatalake_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'storagedatalake_ls'"
		},
		"github_http_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"shreyasgithub_ls_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "skatale@umich.edu"
		},
		"storagedatalake_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shreyassalesdatalake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-sales-shreyas_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"shreyasgithub_ls_credential": {
						"value": "[parameters('shreyasgithub_ls_credential')]"
					},
					"storagedatalake_ls_accountKey": {
						"value": "[parameters('storagedatalake_ls_accountKey')]"
					},
					"github_http_ls_properties_typeProperties_url": {
						"value": "[parameters('github_http_ls_properties_typeProperties_url')]"
					},
					"shreyasgithub_ls_properties_typeProperties_username": {
						"value": "[parameters('shreyasgithub_ls_properties_typeProperties_username')]"
					},
					"storagedatalake_ls_properties_typeProperties_url": {
						"value": "[parameters('storagedatalake_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}